<div class="proList {{="J_proList" + it.key}}" trace='{{=it.key}}'>
    <ul class="clearfix">
        {{~it:pro:index}}
            {{
                var LineType ="";
                if(pro.Property==1)
                    {LineType ="跟团游"}
                else if(pro.Property == 3)
                    { LineType ="自由行"}
                else if(pro.Property == 10)
                    {LineType = "目的地参团"}
                var linkUrl="http://www.ly.com/dujia/tours/" +pro.Id+(pro.LineType==1? ".html": "/")+ "?ak="+pro.RedirectKey+ "&SellType="+pro.SellType;
            }}
            {{?index<6}}
            <li class="product">
                <a class="proline__link" trace="{{=index}}" target="_blank" href="{{=linkUrl}}">
                    <div class="proline__img">
                        <img data-img="{{=pro.ImageUrl}}" title="{{=pro.Title}}" alt="{{=pro.Title}}">
                    </div>
                    <div class="proline__txt">
                        <p class="proline__title" title="{{=pro.Title}}">{{=pro.Title}}</p>
                        <p class="proline__subTitle" title="{{=pro.SubTitle}}">{{=pro.SubTitle}}</p>
                        <p class="proline__price">
                            <span>¥<em>{{=pro.PreferentialPrice}}</em><b> 起</b></span>
                        </p>
                    </div>
                    <span class="startCity">{{=pro.PortCity}}丨{{=LineType}}</span>
                </a>
            </li>
            {{?}}
        {{~}}
    </ul>
</div>
